# Network Configuration
VITE_APP_NETWORK=testnet

# Contract Deployment
# Your testnet wallet address that will deploy the contracts
VITE_MODULE_PUBLISHER_ACCOUNT_ADDRESS=
# Your testnet wallet private key (keep this secure!)
VITE_MODULE_PUBLISHER_ACCOUNT_PRIVATE_KEY=

# Deployed Module Address (auto-populated after deployment)
VITE_MODULE_ADDRESS=

# Stake Pool Configuration (for yield strategies)
# The fungible asset to use for staking (e.g., 0x1::aptos_coin::AptosCoin for APT)
VITE_FA_ADDRESS=0x1::aptos_coin::AptosCoin
# Address authorized to create reward schedules
VITE_REWARD_CREATOR_ADDRESS=

# Protocol Addresses (Testnet)
# Liquidswap DEX
VITE_LIQUIDSWAP_ADDRESS=0xc4d5cb0e84c7dfc3498d38dcbe1a898b694c2a8c274fbc99696b7773fe75b381
# ThalaSwap (verify testnet address)
VITE_THALASWAP_ADDRESS=

# Execution Agent Configuration
# Separate wallet for automated strategy execution
EXECUTION_AGENT_PRIVATE_KEY=
# How often to check for strategies to execute (in milliseconds)
CRON_INTERVAL=60000

# Optional Services
# Aptos API key for better rate limits
VITE_APTOS_API_KEY=
# OpenAI API key for advanced NLP parsing (optional)
OPENAI_API_KEY=

# Development
# Set to true to enable dev mode features
VITE_DEV_MODE=true
